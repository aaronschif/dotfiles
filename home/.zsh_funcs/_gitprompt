# -*- grammar-ext: .zsh -*-
local BIN=/home/aaron/Projects/gitprompt/target/debug/gitprompt

if ! test -f "$BIN"
then
    function _gitprompt { return }
    return
fi

local git=
if git=`$BIN -q -t 300`
then
    echo "%F{blue}GIT%F{green}$git "
fi
