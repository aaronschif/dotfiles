# -*- grammar-ext: .zsh -*-
local BIN=/home/aaron/Projects/dgitprompt/dgitprompt

if ! test -f "$BIN"
then
    function _gitprompt { return }
    return
fi

local git=
if git=`$BIN -q`
then
    echo " %F{blue}GIT%F{green}$git"
fi
